{
  "name": "app",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.46.1",
    "esbuild": "^0.16.15",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-d3-tree": "^3.4.2",
    "react-dom": "^18.2.0",
    "swr": "^2.0.4",
    "tailwindcss": "^3.2.4"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets --loader:.js=jsx",
    "build:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint 'app/javascript/**/*.{js,ts,tsx}' 'spec/javascript/**/*.{js,ts,tsx}'",
    "lint:prettier": "prettier app/javascript/**/*.{js,ts,tsx} --check",
    "prettier": "npx prettier --write app/javascript/**/*.{js,ts,tsx}"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
